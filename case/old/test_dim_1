int g_a[5][5];
int main()
{
   int a[2][2][2][2];
   int b[5];
   
   int e = 2;
   b[1] = 5;
   b[2] = 1;
   write(b[2]);
   write(" 1\n");
   a[1][b[2]-1][1][1]=b[1];
   write(a[1][b[2]-1][1][1]);
   write(" 2\n");
   a[1][b[2]-1][1][1]=b[1]*b[1]-b[1];
   write(a[1][b[2]-1][1][1]);
   write(" 3\n");
   a[1][e-2][1][1]=b[1]*b[1];
   write(a[1][0][1][1]);
   write(" 4\n");
   write(a[1][e-2][1][1]);
   write(" 5\n");
   e = a[1][0][1][1];
   write(e);
   write(" 6\n");
   g_a[3][2] = 20;
   write(g_a[3][2]);
   write(" 7\n");
   return 0;
}
